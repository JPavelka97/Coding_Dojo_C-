@{
    ViewData["Title"] = "Create Chef"; 
}
@model List<Dish>

<div>
    <h1 class="text-center">Add a new Chef!</h1>
    <div class="d-flex gap-3 justify-content-center">
        <a href="/">Home</a>
        <p>|</p>
        <p>Add a Dish</p>
    </div>
    @foreach(Dish d in Model)
    {
        <p>@d.Name created @d.Dish.Name</p>
    }
</div>
